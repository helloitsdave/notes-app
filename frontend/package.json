    "@testing-library/jest-dom": "6.2.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "1.1.3",
    "axios-mock-adapter": "1.22.0",
    "jsdom": "23.0.1",
    "msw": "2.0.11",
    "vitest": "1.1.2",
    "whatwg-fetch": "3.6.20"
  }
